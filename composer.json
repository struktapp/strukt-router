{
    "name": "strukt/router",
    "description": "Strukt Router",
    "license": "MIT",
    "authors": [
        
        {
            "name": "Moderator",
            "email": "pitsolu@gmail.com"
        }
    ],
    "require": {

        "php": "^8.1",
        "strukt/contract": "v1.1.1-alpha",
        "symfony/http-foundation": "7.3.x-dev"
    },
    "require-dev": {

        "phpunit/phpunit":"*",
        "phpcsstandards/phpcsutils": "^1.0@dev",
        "phpcompatibility/php-compatibility": "dev-develop"
    },
    "autoload": {
        
        "psr-0": {

            "Strukt\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev":{

        "psr-0":{

            "App\\": "fixtures/"
        }
    },
    "bin":["bin/static"],
    "minimum-stability":"dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
